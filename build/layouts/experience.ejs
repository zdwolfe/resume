<div class="span12">
    <div id="experience-header" class="row">
        <div class="span12">
            <h2>Employment Experience</h2>
        </div>
    </div>
    <div id="experience-content" class="row">
        <ul class="span12">
            <% for(var job_number=0; job_number < jobs_to_display && job_number < experience.length; job_number++) { %>
            <li class="row">
                <div class="span3">
                    <ul class="employer-info">
                        <li class="employer-name">
                            <%= experience[job_number].employer_name %>
                        </li>
                        <li class="employer-location">
                            <%= experience[job_number].employer_location %>
                        <li>
                        <li class="employer-time">
                            <%= experience[job_number].employer_time %>
                        <li>
                    </ul>
                </div>
                <div class="span8">
                    <div class="row">
                        <div class="span12">
                            <h5 class="job-title">Senior Software Development Engineer Intern</h5>
                            <ul class="job-duties">
                                <% for(var duty_number=0; duty_number < experience[job_number].job_duties.length; duty_number++) { %>
                                <li><%= experience[job_number].job_duties[duty_number]  %></li> 
                                <% } %>
                            </ul>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span12">
                            <ul class="job-skills">
                                <% for(var skill_number=0; skill_number < experience[job_number].job_skills.length; skill_number++) { %>
                                <li><div class="well"><%= experience[job_number].job_skills[skill_number]  %></div></li> 
                                <% } %>
                            </ul>
                        </div>
                    </div>
                </div>
            </li>
            <% } %>
        </ul>
    </div>
</div>
